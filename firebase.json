{
  "database": {
    "rules": "frontend/database.rules.json"
  },
  "hosting": {
    "public": "frontend/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "frontend/build",
        "destination": "index.html"
      }
    ]
  }
}
